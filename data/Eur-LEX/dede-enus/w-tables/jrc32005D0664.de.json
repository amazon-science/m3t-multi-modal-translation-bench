            ],
            "order": "8",
            "label": "Section-header",
            "text": "2. Einzelheiten der Maßnahme",
            "uuid": "0f244c95-d8e4-40cb-9175-a45423587e03",
            "vertical": false,
            "translation": "2. Details of the measure"
